@model IEnumerable<Repozytorium.Models.FundZakup>
@{
    ViewBag.Title = "Main";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Main</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fund.FundName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Uzytkownik.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FundZakupIlosc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FundZakupCena)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FundZakupDataZakupu)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fund.FundName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uzytkownik.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FundZakupIlosc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FundZakupCena)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FundZakupDataZakupu)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.FundZakupID }) |
                @Html.ActionLink("Details", "Details", new { id = item.FundZakupID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.FundZakupID })
            </td>
        </tr>
    }

</table>
